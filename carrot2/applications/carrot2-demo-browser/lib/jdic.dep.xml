<?xml version="1.0" ?>
<!DOCTYPE component PUBLIC "-//Carrot2//DTD Component Dependency Descriptor V1.0//EN" "http://www.carrot2.org/dtd/component-dependency.dtd" >

<component
    name="jdic"
    description="Java Desktop Integration Components (JDIC), Version 20061102 (with patches), https://jdic.dev.java.net/">
    <!--
Minor patch added to avoid spamming System.out.

--- JdicManager.java_orig	2006-10-19 15:40:50.000000000 +0200
+++ JdicManager.java	2007-08-03 15:38:41.250000000 +0200
@@ -262,7 +262,7 @@
 	}
 
 	public String getBinaryPath() {
-		System.out.print("native lib path "+nativeLibPath);
+		org.apache.log4j.Logger.getLogger(JdicManager.class).debug("Native libraries path " + nativeLibPath);
 		return nativeLibPath;
 	}
 
    -->
	<provides>
		<files>
			<file location="jdic.jar" />

			<file location="jdic_windows/IeEmbed.exe" />
			<file location="jdic_windows/jdic.dll" />
			<file location="jdic_windows/jdic_stub.jar" />
			<file location="jdic_windows/MozEmbed.exe" />
			<file location="jdic_windows/tray.dll" />

			<file location="jdic_linux/libjdic.so" />
			<file location="jdic_linux/libmozembed-linux-gtk1.2.so" />
			<file location="jdic_linux/libmozembed-linux-gtk2.so" />
			<file location="jdic_linux/libtray.so" />
			<file location="jdic_linux/mozembed-linux-gtk1.2" />
			<file location="jdic_linux/mozembed-linux-gtk2" />
		</files>
	</provides>
</component>

